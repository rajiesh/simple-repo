{
  "name": "all_caps",
  "environment_variables": [
   {
      "name": "FromConfigRepo",
      "value": "#{ConfigParameter}",
      "secure": false
    }
  ],
  "group" : "newgroup",
  "template" : "config-temp",
  "lock_behavior" : "unlockWhenFinished",
  "materials": [
    {
      "type": "git",
      "name": "git",
      "url": "http://github.com/rajiesh/simple-repo.git",
      "destination": "#{ConfigParameter}"
    }
  ],
  "parameters": [
    {
      "name": "ConfigParameter",
      "value": "ParamFromConfigRepo"
    }
  ]
}
